# Maven Build Cache Configuration for Docker
# This file optimizes Maven builds inside Docker containers

# Skip tests during Docker builds (tests should run in CI/CD)
skipTests=true

# Use offline mode when possible
offline=false

# Parallel builds
threads=1C

# Fail fast on errors
failFast=true

# Don't download sources and javadocs in Docker builds
downloadSources=false
downloadJavadocs=false

# Use local repository optimization
localRepository=/root/.m2/repository
